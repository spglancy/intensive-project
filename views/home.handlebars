            <div class="feed">
                <div class="poster">
                    {{>post-form}}
                </div>
                <div class="posts">
                    {{#each post}}
                        <div class="post">
                            <div class="postContent">
                                <img src="/assets/blankUser.png">
                                <h2>{{this.user}}</h2>
                                <p>{{this.content}}</p>
                            </div>
                            <div class="buttons">
                                <form action="/posts/{{this.id}}/edit">
                                    <button>Edit</button>
                                </form>
                                <form action="/posts/{{this.id}}">
                                    <button>View</button>
                                </form>
                            </div>
                        </div>
                        {{else}}
                        <p>no content</p>
                    {{/each}}
                </div>
            </div>
